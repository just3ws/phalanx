<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% assign pages = site.pages | where_exp: 'p', 'p.url != "/sitemap.xml" and p.url != "/robots.txt"' %}
  {% for p in pages %}
  <url>
    <loc>{{ p.url | absolute_url }}</loc>
  </url>
  {% endfor %}
</urlset>
